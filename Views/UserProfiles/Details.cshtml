@model VisualHead.Models.UserProfile

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>UserProfile</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ApplicationUser)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ApplicationUser.Id)
        </dd>
        <dd class="col-sm-10">
            <h6>
                First Name:
                @Html.DisplayFor(Model => Model.ApplicationUser.FirstName)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>
                Last Name:
                @Html.DisplayFor(Model => Model.ApplicationUser.LastName)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>
                Cellphone Number:
                @Html.DisplayFor(Model => Model.ApplicationUser.PhoneNumber)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>
                Type:
                @Html.DisplayFor(model => model.Type)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>
                Created At:
                @Html.DisplayFor(Model => Model.Month)
            </h6>
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.UniqueUserName">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
